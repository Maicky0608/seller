{
  "name": "seller-center",
  "version": "1.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "config": "ts-node ./scripts/set-env.ts",
    "build": "ng build -c production",
    "start": "ng serve -c stage",
    "development": "ng serve -c local",
    "json-server": "node json-server/server.js  --routes json-server/routes.json",
    "test": "ng test -sm=false",
    "test:ci": "npm run lint -s && ng test --single-run --code-coverage",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "docs": "compodoc -s",
    "docs:hads": "hads ./docs -o",
    "compodoc": "./node_modules/.bin/compodoc -p src/tsconfig.app.json -n \"Seller Center documentation\"",
    "tsc:server": "tsc -p server/tsconfig.ssr.json",
    "build:client-and-server-bundles": "ng build --prod --aot && ng build --prod --aot",
    "build:universal": "npm run build:client-and-server-bundles  && npm run tsc:server",
    "serve:universal": "node deploy/dist-server/index.js"
  },
  "private": true,
